            <w:r>
                <w:t>3、甲方为乙方提供以下福利待遇。</w:t>
            </w:r>
        </w:p>
        <w:p>
            <w:pPr>
                <w:pStyle w:val="9"/>
                <w:keepNext w:val="0"/>
                <w:keepLines w:val="0"/>
                <w:widowControl/>
                <w:suppressLineNumbers w:val="0"/>
